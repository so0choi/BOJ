    "m,n = map(int,input().split())\n",
    "if n>m:\n",
    "    m,n = n,m\n",
    "r = m%n\n",
    "m2 = m\n",
    "n2 = n\n",
    "while True:\n",
    "    r = m%n\n",
    "    if r == 0:\n",
    "        break\n",
    "    m = n\n",
    "    n = r\n",
    "print(n)\n",
    "print((n2//n)*(m2//n)*n)"
   ]
  }
