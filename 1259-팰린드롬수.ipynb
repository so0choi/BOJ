    "while True:\n",
    "    word = input()\n",
    "    if word == '0':\n",
    "        break\n",
    "    n = len(word)-1\n",
    "    for i in range(n//2+1):\n",
    "        if word[i] != word[n-i]:\n",
    "            print('no')\n",
    "            break\n",
    "    else:\n",
    "        print('yes')"
   ]
